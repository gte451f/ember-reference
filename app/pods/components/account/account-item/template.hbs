<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">{{account.name}}</h3>
    </div>
    <div class="panel-body">
        Created On: {{moment-format account.createdOn}}

        <div class="row">
            <div class="col-md-6">
                <h3>Owners</h3>
                <!-- List group -->
                <ul class="list-group">
                    {{#each account.owners as |owner|}}
                        {{account/owner-item owner=owner}}
                    {{/each}}
                </ul>
            </div>
            <div class="col-md-6">
                <h3>Addresses</h3>
                <!-- List group -->
                <ul class="list-group">
                    {{#each account.accountAddrs as |address|}}
                        <li class="list-group-item">
                            {{address/account-address address=address}}
                        </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>